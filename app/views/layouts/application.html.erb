<!DOCTYPE html>
<html>
<head>
  <title>Sitefinal</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
</head>
<body>
<% if action_name == "accueil" || action_name == "about"  || action_name == "index" || action_name == "findex" || action_name == "show" || action_name == "casting" %>
<%= render 'layouts/partials/nav' %>
<% end %>

<% if action_name == "home" || action_name == "indexmovie"  || action_name == "abouten" || action_name == "findexen" || action_name == "showen" || action_name == "cast" %>
<%= render 'layouts/partials/naven' %>
<% end %>

<%= yield %>
</body>
</html>
